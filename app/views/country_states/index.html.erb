<p id="notice"><%= notice %></p>

<h1>Country States</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @country_states.each do |country_state| %>
      <tr>
        <td><%= country_state.name %></td>
        <td><%= link_to 'Show', country_state %></td>
        <td><%= link_to 'Edit', edit_country_state_path(country_state) %></td>
        <td><%= link_to 'Destroy', country_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Country State', new_country_state_path %>
